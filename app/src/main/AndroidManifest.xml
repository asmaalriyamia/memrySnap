<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.alzakwani.memorysnap">

    <application
        android:allowBackup="true"
        android:icon="@drawable/pink"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".user_home.Registeration" android:parentActivityName=".MainActivity"
            android:label="Users Registeration" />
        <activity android:name=".Login" />
        <activity
            android:name=".photographer.Photographer_Registeration" android:parentActivityName=".MainActivity"
            android:label="Photographer Registeration" />
        <activity android:name=".user_home.User_Home" >

    </activity>
        <activity android:name=".user_home.CustomAdapter"></activity>
    </application>

</manifest>